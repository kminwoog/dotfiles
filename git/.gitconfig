[user]
  name = kminwoog
  email = kminwoog@gmail.com
[core]
  commentchar = $
  editor = \"GitExtensions.exe\" fileeditor
  excludesfile = ~/.gitignore_global
  fileMode = false
[merge]
  tool = p4merge
[diff]
  guitool = p4merge
  tool = p4merge
[mergetool "p4merge"]
  cmd = \"p4merge.exe\" \"$BASE\" \"$LOCAL\" \"$REMOTE\" \"$MERGED\"
  path = p4merge.exe
[difftool "p4merge"]
  cmd = \"p4merge.exe\" \"$LOCAL\" \"$REMOTE\"
  path = p4merge.exe
[mergetool]
  trustExitcode = false
  keepBackup = false
[difftool]
  prompt = false
[color]
  ui = auto
[i18n]
  commitEncoding = utf-8
  logOutputEncoding = utf-8
[filter "lfs"]
  clean = git-lfs clean -- %f
  smudge = git-lfs smudge -- %f
  required = true
  process = git-lfs filter-process
[lfs]
  batch = true
  ConcurrentTransfers = 50
[gui]
  encoding = utf-8
[credential]
  helper = manager
[alias]
  co = checkout
  cp = cherry-pick -x
[pull]
  rebase = true
[push]
  default = simple
[fetch]
  prune = true
[rebase]
	autoStash = true
